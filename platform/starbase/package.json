{
  "name": "@kubelt/platform.starbase",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "wrangler publish --dry-run --outdir=dist",
    "check": "run-s format:check lint:check types:check",
    "deploy": "wrangler publish",
    "deploy:dev": "wrangler publish --env dev",
    "dev": "run-s dev:local",
    "dev:local": "run-s types:check wrangler:local",
    "dev:remote": "run-s types:check wrangler:remote",
    "dev:wrangler": "wrangler dev --local --persist",
    "format": "run-s format:src",
    "format:src": "prettier --write src/",
    "format:test": "prettier --write test/",
    "format:check": "run-s 'format:check:*'",
    "format:check:src": "prettier --check src/",
    "lint": "eslint --fix src",
    "lint:check": "run-s 'lint:check:*'",
    "lint:check:src": "eslint src",
    "test": "run-s check",
    "types:check": "tsc --project tsconfig.json --noEmit",
    "wrangler:local": "wrangler dev --local --persist",
    "wrangler:remote": "wrangler dev --env dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20221111.1",
    "@types/itty-router-extras": "0.4.0",
    "@types/lodash": "4.14.189",
    "esbuild": "0.15.15",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tsdoc": "0.2.17",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "typescript": "4.8.4",
    "wrangler": "2.4.4"
  },
  "dependencies": {
    "@ethersproject/bytes": "5.7.0",
    "@kubelt/graph": "workspace:*",
    "@kubelt/openrpc": "workspace:*",
    "@kubelt/security": "workspace:*",
    "@kubelt/urns": "workspace:*",
    "@trpc/server": "10.7.0",
    "cross-env": "7.0.3",
    "do-proxy": "1.3.1",
    "itty-router": "2.6.6",
    "itty-router-extras": "0.4.2",
    "jose": "4.11.0",
    "lodash": "4.17.21",
    "multiformats": "10.0.2",
    "tiny-invariant": "1.3.1",
    "ts-set-utils": "0.2.0"
  }
}
